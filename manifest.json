{
    "manifest_version": 2,
    "name": "Voice Controlled Extension",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "/SOAI.png"
    }
  }
  